      <tr>
        <td>新車時保証書<span><%= car.new_car_warranty ? '◯' : '-' %></span>
        </td>
        <td>スペアタイヤ<span><%= car.spare_tire ? '◯' : '-' %></span>
        </td>
      </tr>
    </table>


