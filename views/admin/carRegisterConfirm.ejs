    <p><a style="display: flex; align-items: center; padding: 0px 20px; border-bottom: 1px solid lightgray; height: 50px; color: black; text-decoration: none; font-size: 16px; font-weight: 600;" href="/admin/cars">未出品車両管理</a></p>
    <p><a style="display: flex; align-items: center; padding: 0px 20px; border-bottom: 1px solid lightgray; height: 50px; color: black; text-decoration: none; font-size: 16px; font-weight: 600;" href="/admin/users">会員管理</a></p>
    <p><a style="display: flex; align-items: center; padding: 0px 20px; height: 50px; color: black; text-decoration: none; font-size: 16px; font-weight: 600;" href="/admin/sales">販売管理</a></p>
</div>
  <div style=" width: 75%;">
<h1 style="font-size: 20px; margin-bottom: 20px;">車両登録確認</h1>
<% for (let i=0; i < car.file.length; i++) { %>
  <img src="/img/tmp/<%= car.file[i].filename %>" alt="" width="170" height="170" style="object-fit: cover; border-radius: 2px;">
  <% } %>
    <table style="margin-bottom: 30px;">
      <tr>
        <td>メーカー名:
          <% if(manufacturer){ %>
            <%= manufacturer[0].manufacture_name %>
              <% } %>
        </td>
        <td>ミッション
          <% if (car.mission=="0" ) {%>
            AT
            <%}else if(car.mission==1){%>
              MT
              <%}%>
        </td>
        <td>走行距離の状態 <%= car.mileage_situation %>
        </td>
      </tr>
      <tr>
        <td>グレード名<%= car.grade %>
        </td>
        <td>色系統
          <% if(color){ %>
            <%= color[0].color_name %>
              <% } %>
        </td>
        <td>輸入車モデル年式<%= car.imported_model_year %>
        </td>
      </tr>
      <tr>
        <td>年式<%= car.model_year %>
        </td>
        <td>車歴<%= car.history %>
        </td>
        <td>色の名称<%= car.color_detail %>
        </td>
      </tr>
      <tr>
        <td>走行距離<%= car.mileage %>
        </td>
        <td>点検記録簿<%= car.Inspection_record_book %>
        </td>
        <td>型式<%= car.model %>
        </td>
      </tr>
      <tr>
        <td>輸入経路<%= car.import_route %>
        </td>
        <td>乗車定員数<%= car.number_passengers %>
        </td>
        <td>リサイクル預託金<%= car.recycling_deposit %>
        </td>
      </tr>
      <tr>
        <td>車台番号（下３桁）<%= car.chassis_number %>
        </td>
        <td>燃料
          <% if (car.fuel=="0" ) {%>
            レギュラー
            <%}else if (car.fuel==1){%>
              ハイオク
              <%}else if (car.fuel==2){%>
                軽油
                <%} %>
        </td>
        <td>ハンドル
          <% if (car.handle=="0" ) {%>
            右
            <%}else if (car.handle==1){%>
              左
              <%} %>
        </td>
      </tr>
      <tr>
        <td>車検有効期限<%= car.car_inspection_expiration_date %>
        </td>
        <td>修復歴<%= car.repair_history %>
        </td>
        <td>所有者歴<%= car.owner_history %>
        </td>
      </tr>
      <tr>
        <td>ボディタイプ
          <% if(bodytype){ %>
            <%= bodytype[0].bodytype_name %>
              <% } %>
        </td>
        <td>排気量<%= car.displacement %>
        </td>
        <td>ドア数<%= car.doors %>
        </td>
      </tr>
      <tr>
        <td>車種名<%= car.car_model_name %>
        </td>
        <td>駆動方式
          <% if (car.drive_system=="0" ) {%>
            FF
            <%}else if (car.drive_system==1){%>
              FR
              <%}else if (car.drive_system==2){%>
                4WD
                <%}else if (car.drive_system==3){%>
                  MR
                  <%}else if (car.drive_system==4){%>
                    RR
                    <%} %>
        </td>
        <td>引き渡し条件<%= car.delivery_conditions %>
        </td>
      </tr>
      <tr>
        <td>車両の状態<%= car.car_condition %>
        </td>
      </tr>
    </table>


