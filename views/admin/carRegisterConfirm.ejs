<style>
  * {
      margin: 0;
      padding: 0;
  }

  header {
      width: 100%;
      height: 50px;
      display: flex;
      align-items: center;
      justify-content: center;
      background-color: #37358C;
      color: white;
  }

  nav {
      width: 90%;
  }

  nav h2 {
      font-size: 20px;
      font-style: italic;
  }

  .aaa p {
      font-size: 16px;
  }

  .aaa h3 {
      font-size: 22px;
  }

  .ccc {
      display: flex;
      justify-content: space-between;
      padding: 10px 0px;
  }
  tr{
    display: flex;
  }
  td{
    padding: 2px;
    font-size: 15px;
    width: 220px;
    display: flex;
    justify-content: space-between;
    padding-right:20px;
  }
  span{
    text-align: right;
  }
  button{
    padding: 5px 40px;
  }
</style>
<header>
  <nav>
      <h2>Kangarooo★</h2>
  </nav>
</header>
<section style="display: flex; justify-content: space-between; margin: 30px 5%;">
